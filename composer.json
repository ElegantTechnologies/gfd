{
    "name": "schooltwist/gfd-core",
    "description": "A bunch of starter types for use with gfd",
    "license": "MIT",
    "authors": [
        {
            "name": "JJBigThoughts",
            "email": "jj.php@rohrer.org"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ramsey/uuid": "^4.1",
        "robinvdvleuten/ulid": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Gfd\\Core\\": "src/Core",
            "Gfd\\Relationships\\": "src/Relationships",
            "Gfd\\Db\\": "src/Db",
            "Gfd\\Utils\\": "src/Utils"
        }

    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
         {
            "type": "vcs",
            "url": "https://bitbucket.org/schooltwist/validations.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
